{
  "name": "webservices-budget",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "web": "node src/index.js",
    "start": "node src/index.js",
    "test": "npx env-cmd -f .env.test jest",
    "test:coverage": "yarn test --coverage",
    "test:ci": "jest"
  },
  "env": {
    "production": true
  },
  "nodemonConfig": {
    "signal": "SIGTERM",
    "ignore": [
      "node_modules/*",
      ".vscode/*"
    ],
    "delay": "80",
    "watch": [
      "config",
      "src"
    ],
    "ext": "js,json",
    "legacyWatch": true,
    "exec": "node --inspect=0.0.0.0:9001 --trace-warnings src/index.js"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.1.1",
    "argon2": "^0.28.3",
    "clean-stack": "^4.1.0",
    "clean-stacktrace": "^1.1.0",
    "config": "^3.3.6",
    "env-cmd": "^10.1.0",
    "install": "^0.13.0",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.12",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "multer": "^1.4.3",
    "mysql2": "^2.3.2",
    "node-emoji": "^1.11.0",
    "serialize-error": "^8.1.0",
    "stack-utils": "^2.0.5",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "supertest": "^6.1.6"
  }
}
